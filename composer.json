{
    "name": "hackr/simplex",
    "description": "Simple practice",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "hackr/keiryo": "v2.x-dev",
        "league/container": "3.*",
        "nette/mail": "*",
        "psr/event-dispatcher": "*",
        "rakit/validation": "*",
        "symfony/config": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/finder": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "robmorgan/phinx": "0.*",
        "tracy/tracy": "2.*",
        "fzaninotto/faker": "1.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Simplex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simplex\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "K. Raphael",
            "email": "raphalogou@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "../keiryo"
        }
    ]
}
